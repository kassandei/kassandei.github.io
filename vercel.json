{
    "version": 2,
    "builds": [
      {
        "src": "api/websocket.js",
        "use": "@vercel/node"
      },
      {
        "src": "public/*",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/",
        "dest": "/public/index.html"
      },
      {
        "src": "/api/websocket",
        "dest": "/api/websocket.js"
      }
    ]
  }
  